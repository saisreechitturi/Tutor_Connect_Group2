# UTA Cloud Production Environment Configuration
# Copy this to .env.production for UTA cloud deployment

# API Configuration
REACT_APP_API_URL=https://

# Production Environment
REACT_APP_ENVIRONMENT=production
REACT_APP_VERSION=1.0.0

# Production Optimizations
GENERATE_SOURCEMAP=false
REACT_APP_DEBUG_MODE=false

# Optional: Production API keys (if needed)
# REACT_APP_MAPS_API_KEY=your-production-maps-key
# REACT_APP_ANALYTICS_ID=your-production-analytics-id

# Optional: Error tracking (if using services like Sentry)
# REACT_APP_SENTRY_DSN=your-sentry-dsn