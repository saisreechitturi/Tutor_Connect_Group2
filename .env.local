# Local Development Environment Configuration
# Copy this to .env.local for local React development

# API Configuration - Local backend
REACT_APP_API_URL=http://localhost:5000/api

# Development Environment
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=1.0.0-dev

# Development Features
REACT_APP_DEBUG_MODE=true
GENERATE_SOURCEMAP=true

# Optional: Local API keys (if needed for development)
# REACT_APP_MAPS_API_KEY=your-dev-maps-key
# REACT_APP_ANALYTICS_ID=your-dev-analytics-id